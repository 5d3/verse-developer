  "version": "1.0.0",
  "manifest_version": 2,
  "content_scripts": [ {
    "js": [ "contentscript.js"],
    "matches": [
      "https://mail.notes.scn2.cn.ibm.com/vers*",
      "https://mail.notes.scniris.com/vers*",
